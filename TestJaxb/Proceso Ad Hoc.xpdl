      <ExtendedAttributes>
        <ExtendedAttribute Name="Objetivos" Value="&lt;p&gt;&lt;span style=&quot;font-family:Segoe UI;font-size:8pt;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;font-family:Segoe UI;font-size:8pt;&quot;&gt;Mostrar la implementaci&amp;#243;n de un proceso Ad Hoc usando la suite de Bizagi. &lt;/span&gt;&lt;/p&gt;&#xA;		&lt;p&gt;&lt;span style=&quot;font-family:Segoe UI;font-size:8pt;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;font-family:Segoe UI;font-size:8pt;&quot;&gt;Implementar un proceso Ad Hoc utilizando el modelo propuesto. &lt;/span&gt;&lt;/p&gt;&#xA;		&lt;p&gt;&amp;nbsp;&lt;/p&gt;" />
        <ExtendedAttribute Name="Alcance_x0020_de_x0020_la_x0020_automatización" Value="&lt;p&gt;&lt;span style=&quot;font-family:Segoe UI;font-size:8pt;&quot;&gt;La plantilla de Procesos Ad Hoc de Bizagi es dise&amp;#241;ada para implementar procesos complejos que necesite la ejecuci&amp;#243;n de tareas que son definidas durante la vida del proceso. La plantilla le permitir&amp;#225; crear y asignar tareas en cualquier momento del proceso y a cualquier persona.  &lt;/span&gt;&lt;/p&gt;" />
        <ExtendedAttribute Name="Duración" Value="&lt;p&gt;&amp;nbsp;&lt;/p&gt;" />
        <ExtendedAttribute Name="Ruta_x0020_del_x0020_proceso" Value="&lt;p&gt;&amp;nbsp;&lt;/p&gt;" />
